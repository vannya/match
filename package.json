{
  "name": "modmemes",
  "version": "1.0.1",
  "description": "modmemes - meme library",
  "main": "index.js",
  "engines": {
    "node": "8.9.3",
    "npm": "5.5.1"
  },
  "scripts": {
    "start": "node index.js",
    "client": "npm run start --prefix client",
    "client-dev": "npm run client-dev --prefix client",
    "server": "nodemon index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "beastmode": "concurrently \"npm run server\" \"npm run client-dev\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Van Tabbert",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "mongoose": "^5.5.11",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0"
  }
}
